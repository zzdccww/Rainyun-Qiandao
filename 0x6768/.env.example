# 雨云账号配置 (必填)
# 雨云用户名
RAINYUN_USER=
# 雨云密码
RAINYUN_PWD=

# 运行配置
# 页面加载超时(秒)
TIMEOUT=15
# 随机延时上限(分钟)
MAX_DELAY=90
# 调试模式（跳过延时）
DEBUG=false

# 基础地址与版本 (可选)
# 日志显示的版本号
APP_VERSION=2.5
# 雨云站点地址
APP_BASE_URL=https://app.rainyun.com
# 雨云 API 基础地址
API_BASE_URL=https://api.v2.rainyun.com
# 登录 Cookie 存储文件
COOKIE_FILE=cookies.json
# 积分兑换比例
POINTS_TO_CNY_RATE=2000

# 验证码与图片下载 (可选)
# 验证码最大重试次数
CAPTCHA_RETRY_LIMIT=5
# 图片下载超时(秒)
DOWNLOAD_TIMEOUT=10
# 图片下载最大重试次数
DOWNLOAD_MAX_RETRIES=3
# 图片下载重试间隔(秒)
DOWNLOAD_RETRY_DELAY=2

# API 请求重试 (可选)
# API 请求超时(秒)
REQUEST_TIMEOUT=15
# API 请求最大重试次数
MAX_RETRIES=3
# API 请求重试间隔(秒)
RETRY_DELAY=2

# ===== 服务器管理功能 (可选) =====
# 雨云 API 密钥（从雨云后台获取：用户中心 -> API 密钥）
# 不填则跳过服务器到期检查和自动续费功能
RAINYUN_API_KEY=

# 自动续费开关 (true/false)
AUTO_RENEW=true

# 续费触发阈值：剩余多少天时触发续费 (默认7天)
RENEW_THRESHOLD_DAYS=7

# 续费白名单：只续费指定的产品ID (逗号分隔，留空则续费所有)
# 产品ID可在雨云后台查看，或从日志中获取
# 示例：RENEW_PRODUCT_IDS=44500,44501
RENEW_PRODUCT_IDS=

# 续费价格兜底值
DEFAULT_RENEW_COST_7_DAYS=2258

# ===== 推送服务 (可选) =====
# 说明：任意服务配置了 key/必要字段后即启用，可同时配置多个
# 一言开关（true/false），设置为 false 可关闭一言追加
HITOKOTO=true
# 控制台输出开关（true/false）
CONSOLE=false
# 跳过推送的标题列表（使用换行分隔）
SKIP_PUSH_TITLE=

# Bark
# bark 推送地址或设备码
BARK_PUSH=
# bark 推送是否存档
BARK_ARCHIVE=
# bark 推送分组
BARK_GROUP=
# bark 推送声音
BARK_SOUND=
# bark 推送图标
BARK_ICON=
# bark 推送时效性
BARK_LEVEL=
# bark 推送跳转 URL
BARK_URL=

# 钉钉机器人
# 钉钉机器人的 DD_BOT_TOKEN
DD_BOT_TOKEN=
# 钉钉机器人的 DD_BOT_SECRET
DD_BOT_SECRET=

# 飞书机器人
# 飞书机器人的 FSKEY
FSKEY=
# 飞书机器人的 FSSECRET（签名密钥）
FSSECRET=

# go-cqhttp
# go-cqhttp 推送地址
GOBOT_URL=
# go-cqhttp 推送目标（user_id=xxx 或 group_id=xxx）
GOBOT_QQ=
# go-cqhttp access_token
GOBOT_TOKEN=

# Gotify
# gotify 地址
GOTIFY_URL=
# gotify 应用 token
GOTIFY_TOKEN=
# gotify 消息优先级
GOTIFY_PRIORITY=0

# iGot
# iGot 聚合推送的 IGOT_PUSH_KEY
IGOT_PUSH_KEY=

# Server酱
# Server酱 PUSH_KEY（兼容旧版与 Turbo 版）
PUSH_KEY=

# PushDeer
# PushDeer 的 PUSHDEER_KEY
DEER_KEY=
# PushDeer 的 PUSHDEER_URL
DEER_URL=

# Synology Chat
# Synology Chat URL
CHAT_URL=
# Synology Chat token
CHAT_TOKEN=

# pushplus
# pushplus 用户令牌
PUSH_PLUS_TOKEN=
# pushplus 群组编码
PUSH_PLUS_USER=
# pushplus 发送模板
PUSH_PLUS_TEMPLATE=html
# pushplus 发送渠道
PUSH_PLUS_CHANNEL=wechat
# pushplus webhook 编码
PUSH_PLUS_WEBHOOK=
# pushplus 回调地址
PUSH_PLUS_CALLBACKURL=
# pushplus 好友令牌/企业微信用户ID
PUSH_PLUS_TO=

# 微加机器人
# 微加机器人用户令牌
WE_PLUS_BOT_TOKEN=
# 微加机器人消息接收者
WE_PLUS_BOT_RECEIVER=
# 微加机器人调用版本
WE_PLUS_BOT_VERSION=pro

# qmsg
# qmsg 酱 QMSG_KEY
QMSG_KEY=
# qmsg 酱 QMSG_TYPE
QMSG_TYPE=

# 企业微信
# 企业微信代理地址
QYWX_ORIGIN=
# 企业微信应用配置（corpId,corpSecret,toUser,agentId[,mediaId]）
QYWX_AM=
# 企业微信机器人 key
QYWX_KEY=

# Telegram
# Telegram 机器人 token
TG_BOT_TOKEN=
# Telegram 用户 ID
TG_USER_ID=
# Telegram API 代理地址
TG_API_HOST=
# Telegram 代理认证参数
TG_PROXY_AUTH=
# Telegram 代理主机
TG_PROXY_HOST=
# Telegram 代理端口
TG_PROXY_PORT=

# 智能微秘书
# 智能微秘书 API Key
AIBOTK_KEY=
# 智能微秘书发送目标类型（room/contact）
AIBOTK_TYPE=
# 智能微秘书目标名称
AIBOTK_NAME=

# SMTP 邮件
# SMTP 服务器地址（如 smtp.exmail.qq.com:465）
SMTP_SERVER=
# SMTP 是否使用 SSL（true/false）
SMTP_SSL=false
# SMTP 邮箱
SMTP_EMAIL=
# SMTP 密码或授权码
SMTP_PASSWORD=
# SMTP 发件人名称
SMTP_NAME=

# PushMe
# PushMe 的 PUSHME_KEY
PUSHME_KEY=
# PushMe 的 PUSHME_URL
PUSHME_URL=

# Chronocat
# Chronocat QQ 号或群号（例如 user_id=123 或 group_id=456）
CHRONOCAT_QQ=
# Chronocat token
CHRONOCAT_TOKEN=
# Chronocat URL
CHRONOCAT_URL=

# 自定义 Webhook
# Webhook 请求地址
WEBHOOK_URL=
# Webhook 请求体
WEBHOOK_BODY=
# Webhook 请求头
WEBHOOK_HEADERS=
# Webhook 请求方法
WEBHOOK_METHOD=
# Webhook Content-Type
WEBHOOK_CONTENT_TYPE=

# Ntfy
# Ntfy 地址
NTFY_URL=
# Ntfy topic
NTFY_TOPIC=
# Ntfy 优先级
NTFY_PRIORITY=3
# Ntfy token
NTFY_TOKEN=
# Ntfy 用户名
NTFY_USERNAME=
# Ntfy 密码
NTFY_PASSWORD=
# Ntfy 动作
NTFY_ACTIONS=

# WxPusher
# WxPusher appToken
WXPUSHER_APP_TOKEN=
# WxPusher 主题ID（英文分号分隔）
WXPUSHER_TOPIC_IDS=
# WxPusher 用户ID（英文分号分隔）
WXPUSHER_UIDS=
